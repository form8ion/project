{
  "name": "@travi/project-scaffolder",
  "description": "opinionated scaffolder for new projects",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/project-scaffolder",
  "bugs": "https://github.com/travi/project-scaffolder/issues",
  "homepage": "https://npm.im/@travi/project-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:md": "remark --quiet . --frail",
    "lint:travis": "travis lint --no-interactive",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:wip": "run-s 'test:integration:base -- --tags \"@wip and not @skip\"'",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @skip and not @wip\"'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.8.4",
    "@travi/eslint-config-travi": "^1.6.18",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "6.26.0",
    "bdd-stdin": "0.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "^3.0.4",
    "commitlint-config-travi": "1.2.31",
    "cucumber": "5.0.2",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.0.0-rc.8",
    "mocha": "5.2.0",
    "mock-fs": "^4.5.0",
    "npm-run-all": "4.1.5",
    "nyc": "12.0.2",
    "remark": "^10.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-travi": "1.1.0",
    "remark-toc": "^5.0.0",
    "rollup": "^0.68.0",
    "sinon": "^6.1.3"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "hoek": "^6.0.0",
    "inquirer": "6.2.0",
    "joi": "^14.0.0",
    "mustache": "3.0.1",
    "mz": "2.7.0",
    "nodegit": "0.23.0",
    "shelljs": "0.8.3",
    "spdx-license-list": "5.0.0",
    "write-yaml": "1.0.0"
  }
}
