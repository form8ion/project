{
  "name": "@travi/project-scaffolder",
  "description": "opinionated scaffolder for new projects",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/project-scaffolder",
  "bugs": "https://github.com/travi/project-scaffolder/issues",
  "homepage": "https://npm.im/@travi/project-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "contributors": [
    "Trevor Richardson <npm@trevorrichardson.me> (https://trevorrichardson.me)"
  ],
  "funding": {
    "url": "https://github.com/sponsors/travi"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark --quiet . --frail",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:engines": "check-engine",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:wip": "run-s 'test:integration:base -- --tags \"@wip and not @skip\"'",
    "pretest:integration": "preview",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @skip and not @wip\"'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.17"
  },
  "files": [
    "lib/",
    "templates/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.14.5",
    "@travi/any": "2.0.16",
    "@travi/babel-preset": "3.0.60",
    "@travi/eslint-config": "1.0.75",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.12",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "check-engine": "1.10.0",
    "codecov": "3.8.2",
    "commander": "8.0.0",
    "commitlint-config-travi": "1.3.16",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "6.0.0",
    "lockfile-lint": "4.6.2",
    "make-dir": "3.1.0",
    "mdast-zone": "4.0.1",
    "mocha": "9.0.1",
    "mock-fs": "5.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "package-preview": "4.0.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.13",
    "remark-toc": "7.2.0",
    "remark-usage": "9.0.0",
    "rollup": "2.52.4",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "11.1.1",
    "testdouble": "3.16.1",
    "unist-util-find": "1.0.2"
  },
  "dependencies": {
    "@form8ion/core": "^1.4.0",
    "@form8ion/overridable-prompts": "^1.1.0",
    "@form8ion/readme": "1.3.0",
    "@form8ion/results-reporter": "^1.1.0",
    "@hapi/hoek": "^9.2.0",
    "@hapi/joi": "^17.0.2",
    "@travi/cli-messages": "1.0.5",
    "deepmerge": "^4.2.2",
    "execa": "^5.0.0",
    "hosted-git-info": "^4.0.0",
    "inquirer": "^8.0.0",
    "mustache": "4.2.0",
    "nodegit": "0.27.0",
    "remark": "^13.0.0",
    "snyk": "^1.530.0",
    "spdx-license-list": "6.4.0",
    "word-wrap": "^1.2.3",
    "write-yaml": "1.0.0"
  },
  "snyk": true
}
