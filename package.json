{
  "name": "@travi/project-scaffolder",
  "description": "opinionated scaffolder for new projects",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/project-scaffolder",
  "bugs": "https://github.com/travi/project-scaffolder/issues",
  "homepage": "https://npm.im/@travi/project-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis lint --no-interactive",
    "test": "run-s lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:wip": "run-s 'test:integration:base -- --tags \"@wip and not @skip\"'",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @skip and not @wip\"'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.5",
    "@travi/eslint-config-travi": "1.6.13",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "bdd-stdin": "0.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.0.1",
    "commitlint-config-travi": "1.2.30",
    "cucumber": "4.2.1",
    "cz-conventional-changelog": "2.1.0",
    "doctoc": "1.3.1",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "1.0.0-rc.2",
    "mocha": "5.2.0",
    "mock-fs": "4.4.2",
    "npm-run-all": "4.1.3",
    "nyc": "11.8.0",
    "rollup": "0.59.2",
    "sinon": "5.0.1"
  },
  "dependencies": {
    "chalk": "2.4.0",
    "inquirer": "5.2.0",
    "mustache": "2.3.0",
    "mz": "2.7.0",
    "nodegit": "0.22.0",
    "shelljs": "0.8.2",
    "spdx-license-list": "4.0.0",
    "write-yaml": "1.0.0"
  }
}
