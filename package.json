{
  "name": "@form8ion/project",
  "description": "opinionated scaffolder for new projects",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "form8ion/project",
  "bugs": "https://github.com/form8ion/project/issues",
  "homepage": "https://npm.im/@form8ion/project",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "contributors": [
    "Trevor Richardson <npm@trevorrichardson.me> (https://trevorrichardson.me)"
  ],
  "funding": {
    "url": "https://github.com/sponsors/travi"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark --quiet . --frail",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:engines": "ls-engines",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps cucumber-js test/integration --require-module @babel/register --publish-quiet --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:wip": "run-s 'test:integration:base -- --tags \"@wip and not @skip\"'",
    "pretest:integration": "preview",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @skip and not @wip\"'",
    "pregenerate:md": "run-s build",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.17"
  },
  "files": [
    "lib/",
    "templates/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.17.0",
    "@cucumber/cucumber": "8.0.0-rc.2",
    "@form8ion/remark-lint-preset": "1.0.13",
    "@travi/any": "2.0.19",
    "@travi/babel-preset": "3.0.77",
    "@travi/eslint-config": "1.0.94",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.16",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "c8": "7.11.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.4.9",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "7.0.4",
    "lockfile-lint": "4.6.2",
    "ls-engines": "0.6.5",
    "make-dir": "3.1.0",
    "mdast-util-from-markdown": "0.8.5",
    "mdast-util-heading-range": "2.1.5",
    "mdast-zone": "4.0.1",
    "mocha": "9.2.0",
    "mock-fs": "5.1.2",
    "npm-run-all": "4.1.5",
    "package-preview": "4.0.0",
    "remark-cli": "9.0.0",
    "remark-toc": "7.2.0",
    "remark-usage": "9.0.0",
    "rollup": "2.67.2",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "13.0.1",
    "testdouble": "3.16.4",
    "unist-util-find": "1.0.2"
  },
  "dependencies": {
    "@form8ion/core": "^1.4.0",
    "@form8ion/overridable-prompts": "^1.1.0",
    "@form8ion/readme": "1.3.0",
    "@form8ion/results-reporter": "^1.1.0",
    "@hapi/hoek": "^9.2.0",
    "@travi/cli-messages": "1.0.5",
    "deepmerge": "^4.2.2",
    "execa": "^5.0.0",
    "hosted-git-info": "^4.0.0",
    "inquirer": "^8.0.0",
    "joi": "^17.1.1",
    "mustache": "4.2.0",
    "nodegit": "0.27.0",
    "spdx-license-list": "6.4.0",
    "word-wrap": "^1.2.3",
    "write-yaml": "1.0.0"
  }
}
